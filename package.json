{
	"name": "nuxt4-formkit-mvp",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"prepare": "husky"
	},
	"dependencies": {
		"@formkit/nuxt": "1.6.9",
		"@formkit/themes": "1.6.9",
		"@formkit/vue": "1.6.9",
		"@nuxtjs/tailwindcss": "6.14.0",
		"nuxt": "4.2.1",
		"vue": "3.5.25",
		"vue-router": "4.6.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"husky": "9.1.7",
		"lint-staged": "16.2.7"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc,vue}": [
			"biome check --write --no-errors-on-unmatched"
		]
	}
}
